{
  "name": "Touken Ranbu Helpe Final",
  "manifest_version": 2,
  "version": "2.3.3",
  "devtools_page": "app/panel/init/init.html",
  "content_security_policy": "default-src 'self' chrome-extension-resource: ; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-eval' https://cdn.bootcss.com;  connect-src *; frame-src *;",
  "web_accessible_resources":
  [
    "*"
  ],
  "background": {
    "scripts": ["vendor/lodash.min.js", "bridge/inject.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://www.dmm.com/*/app_id=825012*"],
      "js": ["bridge/detect.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "static/icon_128.png"
  },
  "permissions": [
    "*://www.dmm.com/*/app_id=825012*",
    "*://*.touken-ranbu.jp/*",
    "http://pre.yaolianqitan.com/*",
    "http://115.28.206.203:3000/*",
    "http://flag.moe/*",
    "notifications",
    "alarms",
    "tabs"
  ]
}
